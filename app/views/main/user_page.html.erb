<div style="background:rgba(232,231,231,0.7);">
<div style="width:100%;">
  <center style="padding-top:20px;">
    <table>
      <tr>
        <td><img src='data:image/jpeg;base64,'
                 style="width:100px; height:100px; margin-left:-20px; margin-right:20px;" class="img-circle"/><td>
        <td style="font-family:Avenir Book">
          <p id="username" style="font-size:1.5em;"><%="닉네임"%></p>
          <p style="font-size:1em;">
            Lv.<span id="level" style="font-size:1em;">99</span>&nbsp;&nbsp;&nbsp;
            <span id="point" style="font-size:1em;"><%="점수"%></span>&nbsp;point
          </p>
          <div class="progress" style="height:100%; width:100%; margin:0px;">
            <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%;">
              100%
            </div>
          </div>
        </td>
      </tr>
    </table>
  </center>
</div>
<br>



<center>  
  <div>
    <p id="toggle_btn" style="font-size:1em; cursor:pointer;">
      <span id="glyphicon" class="glyphicon glyphicon-chevron-down"></span>&nbsp;추가 정보
    </p>
    <div id="user_addition_info">
      <p>E-mail : <span id="email"><%="이메일"%></span></p>
      <p>Date_of_birth : <span id="age"><%="생년월일"%></span></p>
      <p>Gender : <span id="gender"><%="성별"%></span></p>
    </div>
  </div>
  <br>
  
  <!--<script>
    $(function() {
      $("#toggle_btn").click(function() {
        $('#glyphicon').toggleClass("glyphicon-chevron-down");
        $('#glyphicon').toggleClass("glyphicon-chevron-up");
        $("#user_addition_info").toggle("blind", 300);
      });
    });
  </script>-->
  <div>
    <p id="user_tag_btn" style="font-size:1.5em;"><strong># 관심 태그</strong></p>
    <div id="tagcloud">
      <p id="tagnil" style="display:none;">투표한 태그가 없습니다...</p>
    </div>
    
    <% if @tagindex == 1%>
      <script>
        $(function() {
          var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;
          var width_98 = width * 0.98;
          var words = [
            <% for i in(0...@tag_arr.length) %>
              {
                text: "<%=@tag_arr[i]%>", 
                weight: <%=i+1%>,
                handlers : {click: function() {location.href = '/main/post_list/<%=@tag_arr[i]%>';}}
              },
            <% end %>
          ];
          $('#tagcloud').jQCloud(words,{
            width: width_98,
            height: width_98,
            shape: 'rectangular'
          });
        });
      </script>
    <% else %>
      <script>
        $(function() {
          $('#tagnil').show();
        });
      </script>
    <% end %>
  </div>
  <br>
  
  <div>
    <p style="font-size:1.5em;"><strong># 투표한 포스트</strong></p>
    <div style="width:100%; height:10px;"></div>
    <div id="postnil" style="display:none;">
      <p>투표한 포스트가 없습니다...</p>
    </div>
    
    <% if @postindex == 1 %>
      <div class="grid" style="margin-left:0.3em;margin-right:-0.3em;">
        <div class="grid-sizer"></div>
        <div class="gutter-sizer"></div>
        <% for i in (0...@post_arr.length) %>
          <% @postTable = Post.all.where(id: @post_arr[i].to_i)[0] %>
          <a href="/main/post_content/<%=@postTable.id%>" style="color:black; text-decoration:none;">
            <div class="grid-item" style="height:<%= rand(20..25) * 10 %>px;">
              <div class="post_img" style="background-image:url('<%=@postTable.img_url%>');"></div>
              <div class="post_title slide-center"><%=@postTable.subject.to_s%></div>
              <div class="post_tag slide-center">
                <span class="tag-btn">#<%=@postTable.main_tag%></span>&nbsp;
                <% @postTable.tags.take(2).each do |x| %>
                  <span class="tag-btn">#<%=x%></span>
                  &nbsp;
                <% end %>
              </div>
            </div>
          </a>
        <% end %>
      </div>
    <% else %>
      <script>
        $(function() {
          $('#postnil').show();
        });
      </script>
    <% end %>
  </div>
  <script>
    $('.grid').masonry({
      // options
      itemSelector: '.grid-item',
      columnWidth: '.grid-sizer',
      gutter: '.gutter-sizer',
      percentPosition: true
    });
  </script>
  
</center>
</div>